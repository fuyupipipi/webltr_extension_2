{
    "name": "ウェブサイト評価システムサポートツール",
    "description": "ウェブサイト評価システムでの評価フォーム入力をサポートします",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/img/rank_icon_16.png",
            "32": "/img/rank_icon_32.png",
            "48": "/img/rank_icon_48.png",
            "128": "/img/rank_icon_128.png"
        }
    },
    "content_scripts": [
    ],
    "permissions": [
        "tabs"
    ]
}